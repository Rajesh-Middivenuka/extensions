{
    "name": "sweep your cookies and history",
    "version": "1.0",
    "manifest_version": 3,
    "description": "sweep your cookies and history with a click",
    "permissions": ["alarms","browsingData","cookies"],
    "background": {
        "service_worker": "background.js"
      },
      "action": {
        "default_popup": "popup.html"
       
      },
      "host_permissions": [
        "http://*/*",
        "https://*/*"
      ]
    
}